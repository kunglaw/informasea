/*=====================================
=            COMPONENT CSS            =
=====================================*/

body {
    padding-top: 70px;
}

.container {
    width: 1280px !important;
}

.container-fluid {
    padding-right: 9%;
    padding-left: 9%;
    /*max-width: 1366px;*/
}

.slider-container {
    padding-right: 50px;
    padding-left: 50px;
}

.block-with-border {
    border-bottom: 1px dashed #ccc;
}

.small-img-container {
    width: 56px;
    height: 56px;
}

.medium-img-container {
    margin: auto;
    width: 130px;
    height: auto;
}

.custom-h4 {
    margin-top: 5px;
    margin-bottom: 5px;
}

.container-fa {
    width: 56px;
    height: 56px;
    text-align: right;
    margin-top: 10px;
    margin-right: 10px;
}

#header {
    margin-bottom: 25px;
}

.navbar {
    color: #ffffff;
    height: 55px;
    border: none;
}

.navbar-brand .input {
    margin-top: -5px;
}

.navbar-brand {
    padding: 10px 0px;
}

.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus {
    color: #fff;
    background-color: #2064ab;
}

.navbar > .container .navbar-brand,
.navbar > .container-fluid .navbar-brand {
    margin-left: 0;
}

.navbar-default .navbar-nav > li > a {
    font-size: 12px;
    color: #ffffff;
}

.navbar-default .navbar-nav > li > a:hover {
    color: white;
    border-bottom: solid 5px #2064ab;
}

.input-group {
    border-bottom: solid 1px #2a5f9b;
    padding-bottom: 0px;
}

.btn-nav-search {
    background-color: inherit;
    color: #2a5f9b;
}

.navbar-form .form-control {
    border-radius: 0;
    font-size: 12px;
}

.navbar-search-form .form-control {
    color: #ffffff;
    background-color: inherit;
    padding: 0px;
    border: none;
    box-shadow: none;
    transition: border 0.3s;
}

.icon {
    display: inline-block;
    width: 25px;
    height: 25px;
    margin-right: 12px;
}

.large-icon {
    display: inline-block;
    width: 100px;
    height: 100px;
}

.navbar-form .input, .navbar-form select, .navbar-form textarea {
    color: #ffffff;
}

.navbar-form textarea:focus, .navbar-search-form input:focus {
    color: #ffffff;
}


.search-query::-webkit-input-placeholder {
    /*WebKit browsers*/
    color: #2a5f9b;
    padding: 0px;
}

.search-query:-moz-placeholder {
    /*Mozilla Firefox 4 to 18*/
    color: #2a5f9b;
    opacity: 1;
    padding: 0px;
}

.search-query::-moz-placeholder {
    /*Mozilla Firefox 19+*/
    color: #2a5f9b;
    opacity: 1;
    padding: 0px;
}

.search-query:-ms-input-placeholder {
    /*Internet Explorer 10+*/
    color: #ffffff;
    padding: 0px;
}

.btn-login {
    font-size: 12px;
    background-color: #207ec8;
    color : white;
}

.btn-login:hover {
    background-color: #2a5f9b;
}

.dropdown-menu {
    color: #333;
}

.dropdown-menu-blue {
    background-color: #2784d1;
}

.dropdown-menu-blue:hover {
    background-color: #207ec8;
}

.dropdown-menu-blue > li {
    color: #ffffff;
}

.dropdown-menu-blue > li > a:hover,
.dropdown-menu-blue > li > a:focus {
    color: #fff;
    background-color: #2064ab;
}

.dropdown-menu-blue > li > a {
    color: white;
}

#page-notification {
    list-style-type: none;
    padding-left: 0px;
}

#dropdown-notification {
    min-width: 400px;
    padding-top: 0px;
    padding-bottom: 0px;
}

#dropdown-notification .notification-header, #dropdown-notification .notification-footer,
#page-notification .notification-header {
    padding: 8px 10px;
}

#dropdown-notification .notification-body {
    max-height: 300px;
    /*overflow-y: auto;*/
}

#dropdown-notification .notification-footer {
    text-align: center;
    /* position:fixed;
        bottom: 0; */
}

.notification-item {
    padding: 8px 10px;
    text-decoration: none;
    background-color: white;
    border-bottom: 1px solid #f5f5f5;
}

.notification-item:hover {
    cursor: pointer;
    background-color: #f5f5f5;
}

#dropdown-notification > li > div {
    display: block;
    padding: 10px 20px;
    clear: both;
    font-weight: 400;
    line-height: 1.42857;
    font-size: 0.9em;
    color: #333;
    border-bottom: 1px solid #015B55;
    /* white-space: nowrap; */
}

#dropdown-notification > li > div:hover {
    cursor: pointer;
}

.notification-header {
    background-color: #f6f4f5;
}

.navbar-fixed-top {
    background: #3093e4;
}

.nav-custom {
    display: table;
    margin: 0 auto;
}

.nav-custom li {
    display: inline-block;
}

.nav-custom .active a {
    color: #3093e4;
}

.nav-custom li a {
    color: grey;
    font-weight: bold;
}

.main-menu > li > a {
    padding-left : 25px;
    padding-right: 25px;
}

