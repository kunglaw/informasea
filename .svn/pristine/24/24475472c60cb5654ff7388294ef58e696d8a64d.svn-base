/*===================================
=            Media Block            =
===================================*/

@media (min-width: 1366px) {

    #sticky.stick {
        width: 61%;
    }
}

@media (min-width: 1280px) and (max-width: 1366px) {
    .container-fluid {
        padding-right: 100px;
        padding-left: 100px;
    }

    .index-container {
        margin-top: -20px;
    }

    .form-inline-custom .form-group {
        margin-bottom: 0px;
    }
}

@media (max-width: 1280px) {
    .container-fluid {
        padding-right: 20px;
        padding-left: 20px;
    }
}

@media (max-width: 1024px) {
    .container-fluid {
        padding-right: 20px;
        padding-left: 20px;
    }
}

@media (max-width: 768px) {
    body {
        padding-top: 50px;
    }
    
    #sticky.stick {
        width: 94.6%;
    }

    .container-fluid {
        /*margin-top: 10px;*/
        padding: 0 20px;
        overflow: hidden;
    }

    .navbar .container-fluid {
        padding: 0 12px;
        overflow: visible;
    }

    /* #profile-header {
        position: absolute;
        margin-left: -20px;
    } */
    .navbar {
        margin-bottom: 0px;
    }

    .navbar > .container .navbar-brand, .navbar > .container-fluid .navbar-brand {
        margin-left: 15px;
    }

    .dropdown-menu {
        left: -160px;
        top: 35px;
    }

    .nav-mobile {
        list-style-type: none;
        padding-top: 12px;
        padding-left: 12px;
        padding-right: 12px;
    }

    .navbar-default .navbar-brand {
        color: white;
    }

    .nav-mobile li {
        display: inline-block;
        padding: 0px 8px;
    }

    .dropdown-menu-blue, .nav-mobile > li > a {
        color: white;
        font-size: 16px;
    }

    .mobile-list-item {
        position: relative;
    }
    
    .notif-badge {
        left: 15px;
        top: -5px;
    }
    

    .write-status-block {
        margin-top: 25px;
    }

    /* Profile Block */
    /* #profile-header {
        width:50%;
    } */
}

/* Media for Iphone 4S Resolution */
@media (max-width: 375px) {
    #profile-header {
        max-height: 170px;
    }

    #profile-header .media-object {
        max-width: 64px;
    }

    .dropdown-menu-blue {
        max-height: 290px;
        overflow: scroll;
    }

    .media-object {
        max-width: 48px;
    }

    #profile-header .profile-text-container {
        top: 50%;
    }

    .write-status-block {
        margin-top: 25px;
    }
}

