.about-content .container-fluid {
    padding-top: 20px;
}

/*================================
=          resume                =
================================*/

.container-resume {
    padding: 10px;
    border: solid 1px lightgrey;
}

.container-resume thead {
    background-color: #ecf6ff;
    border: solid 1px lightgrey;
}

.container-resume > Table > tbody > tr {
    border: solid 1px lightgrey;
}

.container-resume > table {
    margin-top: 5px;
    padding: 0 10px;
}

.bio-resume {
    margin-top: 5px;
    padding: 20px;
    border: solid 1px lightgrey;
    color: #575757;
}

.container-resume .table-responsive {
    border: none;
}

.bio-resume td {
    padding: 5px;
}

.header-text {
    color: #808080;
}

.container-resume td {
    color: #808080;
}

/*=========================================
=             Landing Page                =
=========================================*/
.landing-container {
    padding-top: 40px;
    padding-bottom: 40px;
    min-height: 300px;
	border-top:1px solid #CCC;
}

.landing-container .section-header, .landing-container .section-footer {
    text-align: center;
}

.search-form {
    padding: 20px;
}

.width-50p {
    width: 50%;
}

.width-40p {
    width: 40%;
}

.box-container {
    border: solid 1px lightgrey;
}

.pd-10 {
    padding: 10px;
}

.grey-container {
    /*background-color: #f7f7f7;*/
	background-color: #f3f3f3;
}

.white-container {
    background-color: #ffffff;
	border:1px solid #CCC;
}

.splitter {
    background-color: #3093e4;
    width: 100px;
    height: 5px;
    margin-top: 0;
}

.block .media img {
    width: 100px;
}

.carousel-control.left {
    background: none;
}

.carousel-control.right {
    right: 0;
    left: auto;
    background: none;
}

.seatizen-card-img-container img {
    max-width: 100px;
    height: auto;
}

.col-text {
    padding: 5px;
    vertical-align: top;
}
.display-block{
    display: block;
}

.border-bottom {
    border-bottom: solid 1px lightgrey;
}

.block .company-logo img {
    width: 100px;
}

.company-container{
    padding-top: 30px;
    padding-bottom: 30px;
	
}

.carousel-testimonial {
    position: relative;
}

