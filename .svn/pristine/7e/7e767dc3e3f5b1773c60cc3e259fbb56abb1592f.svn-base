@charset "utf-8";
/* CSS Document */

@media (max-width: 320px) {
	
	#test_responsive
	{
		visibility:hidden	
	}
	
	/* menu */
	#navbar-main ul li{
		
		font-size:12px;
		/*padding:0px -20px 0px -20px;*/
		margin:0px -8px 0px -8px;	
		
	}
	
	/* timeline*/
	#timeline-content #title-tml p a{
			
		font-size:10px;
		line-height:10px;
		
	}
	
	#timeline-content #title-tml span.glyphicon{
			
		display:none;
		
	}
	
	#timeline-content #title-tml small{
		
		line-height:10px;	
		
	}
	
	#timeline-content #title-tml #dropdownMenu1{
		
		display:none;	
		
	}
	
}


